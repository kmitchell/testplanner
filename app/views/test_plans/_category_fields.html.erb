<fieldset>
  <%= f.label :title, "Category title" %><br />
  <%= f.text_field :title %><br />
  <%= f.check_box :_destroy %>
  <%= f.label :_destroy, "Remove Question" %>
  <%= f.fields_for :test_cases do |testcases| %>
    <%= render 'test_case_fields', f: testcases %>
  <% end %>
  <%= link_to_add_fields "Add Test Case", f, :test_cases %>
</fieldset>
